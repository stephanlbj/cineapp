    "@tanstack/vue-query": "^5.75.5",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "tailwindcss": "^4.1.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "sass-loader": "^10.5.2",
    "stylelint": "^16.19.1",
